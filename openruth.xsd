				<xs:element ref="or:itemHoldings" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="holdingNote" type="xs:string">
		<xs:annotation>
			<xs:documentation xml:lang="en">Note on holding (PIF note)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="illOrder">
		<xs:annotation>
			<xs:documentation xml:lang="en">Details about an ILL order</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="or:itemDisplayTitle"/>
				<xs:element ref="or:itemAuthor" minOccurs="0"/>
				<xs:element ref="or:itemTitle" minOccurs="0"/>
				<xs:element ref="or:itemPublicationYear" minOccurs="0"/>
				<xs:element ref="or:itemSerialPartTitle" minOccurs="0"/>
				<xs:element ref="or:illProviderAgencyId"/>
				<xs:element ref="or:illStatus"/>
				<xs:element ref="or:illOrderDate"/>
				<xs:element ref="or:orderExpectedAvailabilityDate"/>
				<xs:element ref="or:orderId"/>
				<xs:element ref="or:orderDate"/>
				<xs:element ref="or:orderLastInterestDate"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="illOrders">
		<xs:annotation>
			<xs:documentation xml:lang="en">Information about a user's ILL orders</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="or:illOrder" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="illOrderDate" type="xs:date">
		<xs:annotation>
			<xs:documentation xml:lang="en">Date when an ILL order has been placed</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="illProviderAgencyId" type="xs:string">
		<xs:annotation>
			<xs:documentation xml:lang="en">Identifier of provider agency for an ILL</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="illStatus" type="or:illStatusType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Status of an ILL</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="itemAuthor" type="xs:string">
		<xs:annotation>
			<xs:documentation xml:lang="en">Author of an item (bibliographic info)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="itemComingLocation">
		<xs:annotation>
			<xs:documentation xml:lang="en">Availablity information about item not yet available at a given location</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="or:agencyCollectionId"/>
				<xs:element ref="or:bookingAllowed"/>
				<xs:element ref="or:orderAllowed"/>
				<xs:element ref="or:agencyBranchId" minOccurs="0"/>
				<xs:element ref="or:agencyDepartmentId" minOccurs="0"/>
				<xs:element ref="or:agencyPlacementId" minOccurs="0"/>
				<xs:element ref="or:copiesCount"/>
				<xs:element ref="or:itemExpectedDeliveryDate"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="itemDisplayTitle" type="xs:string">
		<xs:annotation>
			<xs:documentation xml:lang="en">Title information (typically a one liner with author, title, publication year) on an item for display purposes</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="itemExpectedDeliveryDate" type="xs:date">
		<xs:annotation>
			<xs:documentation xml:lang="en">Expected date when a not yet available item is ready at a given location</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="itemExpectedAvailabilityDate">
		<xs:annotation>
			<xs:documentation xml:lang="en">Expected date when an item can be available for a user who has not already ordered the material</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="itemHoldings">
		<xs:annotation>
			<xs:documentation xml:lang="en">if itemHoldings is repeated, requested item is a serial</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="or:itemSerialPartId" minOccurs="0"/>
				<xs:element ref="or:itemSerialPartVolume" minOccurs="0"/>
				<xs:element ref="or:itemSerialPartIssue" minOccurs="0"/>
				<xs:element ref="or:ordersCount"/>
				<xs:element ref="or:bookingsCount"/>
				<xs:element ref="or:itemExpectedAvailabilityDate"/>
				<xs:element ref="or:itemLocation" maxOccurs="unbounded"/>
				<xs:element ref="or:itemComingLocation" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="itemId" type="xs:integer">
		<xs:annotation>
			<xs:documentation xml:lang="en">Unique identifier of an item (eg. FAUST number)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="itemLocation">
		<xs:annotation>
			<xs:documentation xml:lang="en">Item availability related to a given collection (repeated for each location)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="or:agencyCollectionId"/>
				<xs:element ref="or:bookingAllowed"/>
				<xs:element ref="or:orderAllowed"/>
				<xs:element ref="or:agencyBranchId" minOccurs="0"/>
				<xs:element ref="or:agencyDepartmentId" minOccurs="0"/>
				<xs:element ref="or:agencyPlacementId" minOccurs="0"/>
				<xs:element ref="or:copiesAvailableCount"/>
				<xs:element ref="or:copiesCount"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="itemPublicationYear" type="xs:string">
		<xs:annotation>
			<xs:documentation xml:lang="en">Publication year of an item (bibliographic info)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="itemSerialPartTitle">
		<xs:annotation>
			<xs:documentation xml:lang="en">Partitle of a serial publication</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="itemSerialPartId" type="xs:integer">
		<xs:annotation>
			<xs:documentation xml:lang="en">Identifier of a part of periodcal or other serial publication. Used in relation with itemId. If 0, empty or not present, item is not part of a serial</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="itemSerialPartIssue" type="xs:string">
		<xs:annotation>
			<xs:documentation xml:lang="en">Issue number of a periodical or other serial publication</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="itemSerialPartVolume" type="xs:string">
		<xs:annotation>
			<xs:documentation xml:lang="en">Volume of a periodical or other serial publication</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="itemTitle" type="xs:string">
		<xs:annotation>
			<xs:documentation xml:lang="en">Title of an item (bibliographic info)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="loan">
		<xs:annotation>
			<xs:documentation xml:lang="en">Details about a loan</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="or:itemDisplayTitle"/>
				<xs:element ref="or:itemAuthor" minOccurs="0"/>
				<xs:element ref="or:itemTitle" minOccurs="0"/>
				<xs:element ref="or:itemPublicationYear" minOccurs="0"/>
				<xs:element ref="or:copyId"/>
				<xs:element ref="or:loanDate"/>
				<xs:element ref="or:loanReturnDate"/>
				<xs:element ref="or:loanLastRenewedDate" minOccurs="0"/>
				<xs:element ref="or:loanStatus" minOccurs="0"/>
				<xs:element ref="or:loanRecallType" minOccurs="0"/>
				<xs:element ref="or:loanRecallDate" minOccurs="0"/>
				<xs:element ref="or:loanRenewable"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="loanCategories">
		<xs:annotation>
			<xs:documentation xml:lang="en">Information about a user's loans within different categories (eg. types of materials)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="or:loanCategory"/>
				<xs:element ref="or:loanCategoryCount"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="loanCategory" type="xs:string">
		<xs:annotation>
			<xs:documentation xml:lang="en">Loan category (eg. types of materials) used in relation with loanCategoryCount</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="loanCategoryCount" type="xs:integer">
		<xs:annotation>
			<xs:documentation xml:lang="en">Number of loans within a loanCategory</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="loanDate" type="xs:date">
		<xs:annotation>
			<xs:documentation xml:lang="en">Date when an item has been lent by user</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="loanLastRenewedDate" type="xs:date">
		<xs:annotation>
			<xs:documentation xml:lang="en">The last time a given loan has been renewed by user</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="loanRecallDate" type="xs:date">
		<xs:annotation>
			<xs:documentation xml:lang="en">Date when a loan has been recalled from user</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="loanRecallType" type="or:loanRecallTypeType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Type of recall</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="loanRecallTypeCount" type="xs:integer">
		<xs:annotation>
			<xs:documentation xml:lang="en">The number of recalls of a given type</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="loanRecallTypes">
		<xs:annotation>
			<xs:documentation xml:lang="en">Information about recalls on user's loans</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="or:loanRecallType"/>
				<xs:element ref="or:loanRecallTypeCount"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="loanRenewable" type="or:loanRenewableType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Indicator whether a loan is renewable</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="loanReturnDate" type="xs:date">
		<xs:annotation>
			<xs:documentation xml:lang="en">Loan due date</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="loanStatus" type="xs:string">
		<xs:annotation>
			<xs:documentation xml:lang="en">The status of a loan (type is locally defined)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="loans">
		<xs:annotation>
			<xs:documentation xml:lang="en">Information about a user's loans</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="or:renewAllLoansAllowed"/>
				<xs:element ref="or:loanCategories" maxOccurs="unbounded"/>
				<xs:element ref="or:loanRecallTypes" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="or:loan" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="orderActivePeriod" type="xs:integer">
		<xs:annotation>
			<xs:documentation xml:lang="en">Number of days that a reservation is active at a given agency</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="orderAllowed" type="xs:boolean">
		<xs:annotation>
			<xs:documentation xml:lang="en">Boolean indicating whether reservation of an item is allowed</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="orderArrived" type="xs:boolean">
		<xs:annotation>
			<xs:documentation xml:lang="en">Boolean indicating whether an order is available for pickup</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="orderDate" type="xs:date">
		<xs:annotation>
			<xs:documentation xml:lang="en">Date when an order has been created in local system</xs:documentation>
		</xs:annotation>
	</xs:element>
