	<xs:element name="fineAmountPaid" type="xs:decimal">
		<xs:annotation>
			<xs:documentation xml:lang="en">Amount paid of a fine</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="fineDate" type="xs:date">
		<xs:annotation>
			<xs:documentation xml:lang="en">Date that a fine has been created in local system</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="fineInvoiceNumber" type="xs:string">
		<xs:annotation>
			<xs:documentation xml:lang="en">Invoice number of a fine</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="fineType" type="or:fineTypeType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Type of fine</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="fines">
		<xs:annotation>
			<xs:documentation xml:lang="en">Information about a user's fines</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="or:fine" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="holding">
		<xs:annotation>
			<xs:documentation xml:lang="en">Holdings on a given item (used in holdingsResponse)</xs:documentation>
