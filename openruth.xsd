			<xs:sequence>
				<xs:element ref="or:itemDisplayTitle"/>
				<xs:element ref="or:itemAuthor" minOccurs="0"/>
				<xs:element ref="or:itemTitle" minOccurs="0"/>
				<xs:element ref="or:itemPublicationYear" minOccurs="0"/>
				<xs:element ref="or:itemSerialPartTitle" minOccurs="0"/>
				<xs:element ref="or:orderId"/>
				<xs:element ref="or:orderDate"/>
				<xs:element ref="or:orderLastInterestDate"/>
				<xs:element ref="or:orderPriority" minOccurs="0"/>
				<xs:element ref="or:orderQuePosition"/>
				<xs:element ref="or:agencyCounter"/>
				<xs:element ref="or:orderNote" minOccurs="0"/>
				<xs:element ref="or:orderType"/>
				<xs:element ref="or:orderExpectedAvailabilityDate" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
