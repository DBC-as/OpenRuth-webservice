	</xs:element>
	<xs:element name="orderId" type="xs:dateTime">
		<xs:annotation>
			<xs:documentation xml:lang="en">dateTime stamp serving as an unique local identifier of an order</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="orderItem">
		<xs:annotation>
			<xs:documentation xml:lang="en">Return information for orderItemResponse</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="or:orderItemId"/>
				<xs:choice>
					<xs:element ref="or:orderItemOk"/>
					<xs:element ref="or:orderItemError"/>
				</xs:choice>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="orderItemError" type="or:orderItemErrorType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Errors related to orderItemRequest</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="orderItemId">
		<xs:annotation>
			<xs:documentation xml:lang="en">Combination of itemId and optional serialPartId used when ordering items or serialParts</xs:documentation>
