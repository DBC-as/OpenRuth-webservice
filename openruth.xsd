		<xs:complexType>
			<xs:sequence>
				<xs:element ref="or:itemId"/>
				<xs:element ref="or:itemSerialPartId" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="orderItemOk">
		<xs:annotation>
			<xs:documentation xml:lang="en">Indicator that orderItemRequest is accepted</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="orderLastInterestDate" type="xs:date">
		<xs:annotation>
			<xs:documentation xml:lang="en">User's last day of interest in an order</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="orderNote" type="xs:string">
		<xs:annotation>
			<xs:documentation xml:lang="en">Note on an order</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="orderOverRule" type="xs:boolean">
