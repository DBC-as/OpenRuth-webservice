			<xs:documentation xml:lang="en">Information about orders</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="or:ordersReady" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="or:ordersNotReady" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ordersCount" type="xs:integer">
		<xs:annotation>
			<xs:documentation xml:lang="en">Number of reservations on a given item</xs:documentation>
		</xs:annotation>
	</xs:element>
