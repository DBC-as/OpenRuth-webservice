<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:or="http://oss.dbc.dk/ns/openruth" targetNamespace="http://oss.dbc.dk/ns/openruth" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="agencyCountersRequest">
		<xs:annotation>
			<xs:documentation xml:lang="en">The service request for information about an agency's counters</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="or:agencyId"/>
				<xs:element ref="or:callback" minOccurs="0"/>
				<xs:element ref="or:outputType" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="agencyCountersResponse">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="or:agencyError"/>
				<xs:element ref="or:agencyCounters"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="bookingInfoRequest">
		<xs:annotation>
			<xs:documentation xml:lang="en">The service request for information about number of copies of an item available at various times. If an active bookingId is included the number of copies included in that booking will be taken into count.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="or:agencyId"/>
				<xs:element ref="or:itemId"/>
				<xs:element ref="or:itemSerialPartId" minOccurs="0"/>
				<xs:element ref="or:bookingId" minOccurs="0"/>
				<xs:element ref="or:callback" minOccurs="0"/>
				<xs:element ref="or:outputType" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="bookingInfoResponse">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="or:bookingInfo"/>
				<xs:element ref="or:bookingError"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="bookItemRequest">
		<xs:annotation>
			<xs:documentation xml:lang="en">The service request for booking an item</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="or:agencyId"/>
				<xs:element ref="or:userId"/>
				<xs:element ref="or:agencyCounter"/>
				<xs:element ref="or:bookingNote"/>
				<xs:element ref="or:bookingStartDate"/>
				<xs:element ref="or:bookingEndDate"/>
				<xs:element ref="or:bookingTotalCount"/>
				<xs:element ref="or:itemId"/>
				<xs:element ref="or:itemSerialPartId" minOccurs="0"/>
				<xs:element ref="or:callback" minOccurs="0"/>
				<xs:element ref="or:outputType" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="bookItemResponse">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="or:bookingOk"/>
				<xs:element ref="or:bookingError"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="cancelBookingRequest">
		<xs:annotation>
			<xs:documentation xml:lang="en">The service request for cancelling a booking of an item</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="or:agencyId"/>
				<xs:element ref="or:bookingId"/>
				<xs:element ref="or:callback" minOccurs="0"/>
				<xs:element ref="or:outputType" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="cancelBookingResponse">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="or:bookingOk"/>
				<xs:element ref="or:bookingError"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="cancelOrderRequest">
		<xs:annotation>
			<xs:documentation xml:lang="en">The service request for deleting a reservation</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="or:agencyId"/>
				<xs:element ref="or:orderId"/>
				<xs:element ref="or:callback" minOccurs="0"/>
				<xs:element ref="or:outputType" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="cancelOrderResponse">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="or:cancelOrderOk"/>
				<xs:element ref="or:cancelOrderError"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="holdingsRequest">
		<xs:annotation>
			<xs:documentation xml:lang="en">The service request for holdings information (agency info, location, availability etc.) about an given item. </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="or:agencyId"/>
				<xs:element ref="or:showComingCopies" minOccurs="0"/>
				<xs:element ref="or:itemId" maxOccurs="unbounded"/>
				<xs:element ref="or:callback" minOccurs="0"/>
				<xs:element ref="or:outputType" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="holdingsResponse">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="or:holding" maxOccurs="unbounded"/>
				<xs:element ref="or:agencyError"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="orderItemRequest">
		<xs:annotation>
			<xs:documentation xml:lang="en">The service request for making a reservation in the local system</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="or:agencyId"/>
				<xs:element ref="or:userId"/>
				<xs:element ref="or:orderNote" minOccurs="0"/>
				<xs:element ref="or:orderLastInterestDate"/>
				<xs:element ref="or:agencyCounter"/>
				<xs:element ref="or:orderOverRule"/>
				<xs:element ref="or:orderPriority"/>
				<xs:element ref="or:orderEachItem" minOccurs="0"/>
				<xs:element ref="or:orderItemId" maxOccurs="unbounded"/>
				<xs:element ref="or:callback" minOccurs="0"/>
				<xs:element ref="or:outputType" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="orderItemResponse">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="or:orderItem" maxOccurs="unbounded"/>
				<xs:element ref="or:orderItemError"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="renewLoanRequest">
		<xs:annotation>
			<xs:documentation xml:lang="en">The service request for renewing one or more loans</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="or:agencyId"/>
				<xs:element ref="or:userId"/>
				<xs:element ref="or:copyId" maxOccurs="unbounded"/>
				<xs:element ref="or:callback" minOccurs="0"/>
				<xs:element ref="or:outputType" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="renewLoanResponse">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="or:renewLoan" maxOccurs="unbounded"/>
				<xs:element ref="or:renewLoanError"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="updateBookingRequest">
		<xs:annotation>
			<xs:documentation xml:lang="en">The service request for updating details about a booking</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="or:agencyId"/>
				<xs:element ref="or:bookingId"/>
				<xs:element ref="or:agencyCounter" minOccurs="0"/>
				<xs:element ref="or:bookingNote" minOccurs="0"/>
				<xs:element ref="or:bookingStartDate" minOccurs="0"/>
				<xs:element ref="or:bookingEndDate" minOccurs="0"/>
				<xs:element ref="or:bookingsCount" minOccurs="0"/>
				<xs:element ref="or:callback" minOccurs="0"/>
				<xs:element ref="or:outputType" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="updateBookingResponse">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="or:bookingOk"/>
				<xs:element ref="or:bookingError"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="updateOrderRequest">
		<xs:annotation>
			<xs:documentation xml:lang="en">The service request for updating details about a reservation in the local system</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="or:agencyId"/>
				<xs:element ref="or:orderId"/>
				<xs:element ref="or:orderNote" minOccurs="0"/>
				<xs:element ref="or:orderLastInterestDate" minOccurs="0"/>
				<xs:element ref="or:agencyCounter" minOccurs="0"/>
				<xs:element ref="or:callback" minOccurs="0"/>
				<xs:element ref="or:outputType" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="updateOrderResponse">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="or:updateOrderOk"/>
				<xs:element ref="or:updateOrderError"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="updateUserInfoRequest">
		<xs:annotation>
			<xs:documentation xml:lang="en">The service request for changing userinfo (pincode, contact, preferences etc.)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="or:agencyId"/>
				<xs:element ref="or:userId"/>
				<xs:element ref="or:userPinCodeOld" minOccurs="0"/>
				<xs:element ref="or:userPinCodeNew" minOccurs="0"/>
				<xs:element ref="or:userEmail" minOccurs="0"/>
				<xs:element ref="or:userMobilePhone" minOccurs="0"/>
				<xs:element ref="or:userPreReturnReminder" minOccurs="0"/>
				<xs:element ref="or:userFirstName" minOccurs="0"/>
				<xs:element ref="or:userLastName" minOccurs="0"/>
				<xs:element ref="or:agencyCounter" minOccurs="0"/>
				<xs:element ref="or:callback" minOccurs="0"/>
				<xs:element ref="or:outputType" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="updateUserInfoResponse">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="or:updateUserInfoOk"/>
				<xs:element ref="or:userError"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="userCheckRequest">
		<xs:annotation>
			<xs:documentation xml:lang="en">The service request for performing a user check (whether the user exists in the system and user's various rights and parameters)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="or:agencyId"/>
				<xs:element ref="or:userId"/>
				<xs:element ref="or:userPinCode"/>
				<xs:element ref="or:callback" minOccurs="0"/>
				<xs:element ref="or:outputType" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="userCheckResponse">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="or:userCheck"/>
				<xs:element ref="or:userError"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="userPaymentRequest">
		<xs:annotation>
			<xs:documentation xml:lang="en">The service request for paying user fines</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="or:agencyId"/>
				<xs:element ref="or:userId"/>
				<xs:element ref="or:fineAmountPaid"/>
				<xs:element ref="or:callback" minOccurs="0"/>
				<xs:element ref="or:outputType" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="userPaymentResponse">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="or:userPaymentOk"/>
				<xs:element ref="or:userPaymentError"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="userStatusRequest">
		<xs:annotation>
			<xs:documentation xml:lang="en">The service reqeust for a user's status, includings loans, orders, ILLs and fines</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="or:agencyId"/>
				<xs:element ref="or:userId"/>
				<xs:element ref="or:userPinCode"/>
				<xs:element ref="or:callback" minOccurs="0"/>
				<xs:element ref="or:outputType" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="userStatusResponse">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="or:userStatus"/>
				<xs:element ref="or:userError"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="agencyBranchId">
		<xs:annotation>
			<xs:documentation xml:lang="en">Identifier of a branch</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="or:agencyBranchCode"/>
				<xs:element ref="or:agencyBranchName"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="agencyBranchCode" type="xs:string">
		<xs:annotation>
			<xs:documentation xml:lang="en">Branch code</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="agencyBranchName" type="xs:string">
		<xs:annotation>
			<xs:documentation xml:lang="en">Name of a branch</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="agencyCollectionId">
		<xs:annotation>
			<xs:documentation xml:lang="en">Identifier of a collection</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="or:agencyCollectionCode"/>
				<xs:element ref="or:agencyCollectionName"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="agencyCollectionCode" type="xs:string">
		<xs:annotation>
			<xs:documentation xml:lang="en">Collection code</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="agencyCollectionName" type="xs:string">
		<xs:annotation>
			<xs:documentation xml:lang="en">Name of a collection</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="agencyCounter" type="xs:string">
		<xs:annotation>
			<xs:documentation xml:lang="en">Identifier of a service counter at a given agency</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="agencyCounterName" type="xs:string">
		<xs:annotation>
			<xs:documentation xml:lang="en">Full name of an agencyCounter</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="agencyCounters">
		<xs:annotation>
			<xs:documentation xml:lang="en">Counters related to a given agencyId</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="or:agencyId"/>
				<xs:element ref="or:agencyName"/>
				<xs:element ref="or:orderActivePeriod"/>
				<xs:element ref="or:agencyCounterInfo" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="agencyCounterInfo">
		<xs:annotation>
			<xs:documentation xml:lang="en">Information about a counter</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="or:agencyCounter"/>
				<xs:element ref="or:agencyCounterName"/>
				<xs:element ref="or:agencyDefaultCounter"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="agencyDefaultCounter" type="xs:boolean">
		<xs:annotation>
			<xs:documentation xml:lang="en">Boolean indicating whether a counter is default service counter at a given agency</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="agencyDepartmentId">
		<xs:annotation>
			<xs:documentation xml:lang="en">Identifier of a department</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="or:agencyDepartmentCode"/>
				<xs:element ref="or:agencyDepartmentName"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="agencyDepartmentCode" type="xs:string">
		<xs:annotation>
			<xs:documentation xml:lang="en">Department code</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="agencyDepartmentName" type="xs:string">
		<xs:annotation>
			<xs:documentation xml:lang="en">Name of a department</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="agencyError" type="or:agencyErrorType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Error messages related to requests for agency information</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="agencyFullName" type="xs:string">
		<xs:annotation>
			<xs:documentation xml:lang="en">Full name of a given agency</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="agencyHoldings">
		<xs:annotation>
			<xs:documentation xml:lang="en">Agency related info used in holdingsResponse</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="or:agencyName"/>
				<xs:element ref="or:agencyFullName"/>
				<xs:element ref="or:agencyId"/>
				<xs:element ref="or:copiesAvailableTotal"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="agencyId" type="xs:string">
		<xs:annotation>
			<xs:documentation xml:lang="en">Unique identifier of an agency</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="agencyName" type="xs:string">
		<xs:annotation>
			<xs:documentation xml:lang="en">Name of a given agency</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="agencyPlacementId">
		<xs:annotation>
			<xs:documentation xml:lang="en">Identifier of a placement</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="or:agencyPlacementCode"/>
				<xs:element ref="or:agencyPlacementName"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="agencyPlacementCode" type="xs:string">
		<xs:annotation>
			<xs:documentation xml:lang="en">Placement code</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="agencyPlacementName" type="xs:string">
		<xs:annotation>
			<xs:documentation xml:lang="en">Placement name</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="booking">
		<xs:annotation>
			<xs:documentation xml:lang="en">Details about a booking</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="or:bookingId"/>
				<xs:element ref="or:itemDisplayTitle"/>
				<xs:element ref="or:bookingTotalCount"/>
				<xs:element ref="or:bookingFetchedCount"/>
				<xs:element ref="or:bookingLoanedCount"/>
				<xs:element ref="or:bookingReturnedCount"/>
				<xs:element ref="or:bookingStartDate"/>
				<xs:element ref="or:bookingEndDate"/>
				<xs:element ref="or:bookingStatus"/>
				<xs:element ref="or:agencyCounter"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="bookingAllowed" type="xs:boolean">
		<xs:annotation>
			<xs:documentation xml:lang="en">Boolean indicating whether booking of a given item is allowed</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="bookingChange">
		<xs:annotation>
			<xs:documentation xml:lang="en">Changes in a potential booking, used in bookingInfoResponse</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="or:bookingChangeCount"/>
				<xs:element ref="or:bookingChangeDate"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="bookingChangeCount" type="xs:integer">
		<xs:annotation>
			<xs:documentation xml:lang="en">Change in number of copies available for booking (related to bookingChangeDate)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="bookingChangeDate" type="xs:date">
		<xs:annotation>
			<xs:documentation xml:lang="en">Date when number of available copies available for booking changes (related to bookingChangeCount)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="bookingEndDate" type="xs:date">
		<xs:annotation>
			<xs:documentation xml:lang="en">Date when a booking expires</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="bookingError" type="or:bookingErrorType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Error related to bookings</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="bookingFetchedCount" type="xs:integer">
		<xs:annotation>
			<xs:documentation xml:lang="en">Number of copies fetched of a booked item</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="bookingId" type="xs:dateTime">
		<xs:annotation>
			<xs:documentation xml:lang="en">dateTime stamp serving as a unique local identifier of a booking</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="bookingInfo">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="or:bookingInfoTotalCount"/>
				<xs:element ref="or:bookingChange" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="bookingInfoTotalCount" type="xs:integer">
		<xs:annotation>
			<xs:documentation xml:lang="en">Total number of copies available for booking</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="bookingLoanedCount" type="xs:integer">
		<xs:annotation>
			<xs:documentation xml:lang="en">Number of copies of a given booking already on loan by user</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="bookingNote" type="xs:string">
		<xs:annotation>
			<xs:documentation xml:lang="en">Text note related to a booking</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="bookingOk" type="xs:string">
		<xs:annotation>
			<xs:documentation xml:lang="en">Message that bookItemRequest has been accepted</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="bookingReturnedCount" type="xs:integer">
		<xs:annotation>
			<xs:documentation xml:lang="en">Number of copies of a given booking that user has returned</xs:documentation>
		</xs:annotation>
	</xs:element>
