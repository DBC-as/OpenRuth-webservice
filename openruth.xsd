	</xs:element>
	<xs:element name="copiesCount" type="xs:integer">
		<xs:annotation>
			<xs:documentation xml:lang="en">Number of copies held at a given location</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="copyId" type="xs:integer">
		<xs:annotation>
			<xs:documentation xml:lang="en">Unique identifier of a copy of an item</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="fine">
